# .env.production
# Production environment variables
# This file should NOT be committed to version control with real credentials
# Make a copy of this file as .env.production.local for actual production values

# API Configuration
VITE_API_BASE_URL=/
VITE_API_TIMEOUT=60000

# Authentication Configuration
VITE_USE_JWT_AUTH=true
# Only used if USE_JWT_AUTH is false
VITE_API_KEY=

# File Upload Configuration
VITE_MAX_FILE_SIZE=100000000
VITE_ACCEPTED_FILE_TYPES=.csv,.xls,.xlsx
VITE_UPLOAD_CHUNK_SIZE=2000000

# UI Configuration
VITE_DEFAULT_THEME=system
VITE_SIDEBAR_COLLAPSED=true
VITE_DASHBOARD_REFRESH=0

# Feature Flags
VITE_ENABLE_USER_MANAGEMENT=true
VITE_ENABLE_DARK_MODE=true
VITE_ENABLE_LOGGING=true

# Environment
NODE_ENV=production